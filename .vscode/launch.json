    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: 当前文件",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "supervision",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--deep_supervision","true"]
        },
        {
            "name": "weight_loss",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--weight_loss","true", "--loss", "WeightBCEDiceLoss"]
        },
        {
            "name": "loss",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--weight_loss","false", "--loss", "BCEDiceLoss"]
        },
        {
            "name": "PyramidUNet",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--arch","PyramidUNet"]
        },
        {
            "name": "batchs",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--batch-size","10"]
        },
        {
            "name": "dataset",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--data_dir","dsb2018_256"]
        },
    ]
